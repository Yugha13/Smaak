generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = "postgres://ashwin:root@localhost:5432/sample1"
}

model Food {
  id        Int @id @default(autoincrement())
  name      String
  price     Float
}